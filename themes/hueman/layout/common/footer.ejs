<footer id="footer">
    <div class="container">
        <div class="container-inner">
            <a id="back-to-top" href="javascript:;"><i class="icon fa fa-angle-up"></i></a>
            <div class="credit">
                <h1 class="logo-wrap">
                    <a href="<%- url_for() %>" class="logo"></a>
                </h1>
                <p>&copy; <%= date(new Date(), 'YYYY') %> <%= config.author || config.title %></p>
                <% if (theme.poweredby) { %>
                <p>Powered by <a href="<%= config.url %>" target="_blank"><%= config.title %></a>.
                <% } %>
            </div>
            <div class="footer-plugins">
            <% if (site.posts.length) { %>
                <% for (var i in theme.miscellaneous.foot_links) { %>
                    <span>
                        <a href="<%- theme.miscellaneous.foot_links[i] %>"><%= i %></a>
                    </span> 
                <% } %>
              <% } %>

              <%- partial('plugin/scripts', { isFooter: true }) %>
            </div>

        </div>
    </div>
</footer>
